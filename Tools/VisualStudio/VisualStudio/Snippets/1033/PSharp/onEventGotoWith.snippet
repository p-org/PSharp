<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>on event goto with</Title>
            <Shortcut>onevgow</Shortcut>
            <Description>Code snippet for a "on event goto with" definition</Description>
            <Author>Microsoft Corporation</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Code Language="PSharp"><![CDATA[on $EventName$ goto $StateName$ with
{
    $end$
}]]>
            </Code>
            <Declarations>
                <Literal>
                    <ID>EventName</ID>
                    <ToolTip>Replace with the name of the event</ToolTip>
                    <Default>EventName</Default>
                </Literal>
                <Literal>
                    <ID>StateName</ID>
                    <ToolTip>Replace with the name of the state</ToolTip>
                    <Default>StateName</Default>
                </Literal>
            </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>