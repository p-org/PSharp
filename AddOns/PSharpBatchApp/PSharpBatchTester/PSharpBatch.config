<?xml version="1.0"?>
<PSharpBatchConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!-- Azure Batch Credentials -->
  <BatchAccountName>$Batch Account Name$</BatchAccountName>
  <BatchAccountKey>$Batch Account Key$</BatchAccountKey>
  <BatchAccountUrl>$Batch Account URL$</BatchAccountUrl>
  <!-- Azure Storage Account Credentials -->
  <StorageAccountName>$Storage Account Name$</StorageAccountName>
  <StorageAccountKey>$storage Account Key</StorageAccountKey>

  <!-- Id of the Azure Batch pool to be created -->
  <PoolId>$Pool ID$</PoolId>
  
  <!-- Prefix of the Job ID. Will be appended by timestamp -->
  <JobDefaultId>$Job Prefix$</JobDefaultId>
  
  <!-- Maximum iterations per task -->
  <MaxIterationPerTask>$Max Iterations per task$</MaxIterationPerTask>
  
  <!-- Total iterations for the testing -->
  <TotalIterations>$Total Iterations$</TotalIterations>

  <!-- Prefix of the task id. Will be appended by timestamp -->  
  <TaskDefaultId>$Task Prefix$</TaskDefaultId>
  
  <!-- Blobl access key expiry hours. Enter -1 for infinite expiry hours. -->
  <BlobContainerSasExpiryHours>$Blob container expiry hours$</BlobContainerSasExpiryHours>

  <!-- Number of nodes to be created -->
  <NumberOfNodesInPool>$Number of nodes in pool$</NumberOfNodesInPool>

  <!-- Path to the PSharp Binaries folder -->
  <PSharpBinariesFolderPath>$Path to the PSharp Binaries folder$</PSharpBinariesFolderPath>
  
  <!-- Path to the Application to be tested -->
  <TestApplicationPath>$Test Application Path$</TestApplicationPath>

  <!-- Path to the output folder -->
  <OutputFolderPath>$Output folder path$</OutputFolderPath>

  <!-- Wait time for monitoring tasks, beyond which the tasks will be terminated -->
  <TaskWaitHours>$Task wait time in hours$</TaskWaitHours>
</PSharpBatchConfig>